<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byType">

    <bean id="autowireremoveambiguities"
          class="com.spring.autowire.removeambiguities.Logger5"
          autowire="byType">
    </bean>

    <bean id="consolewriter"
          class="com.spring.autowire.removeambiguities.ConsoleWriter"
          primary="true">
    </bean>

    <bean id="panda"
          class="com.spring.autowire.removeambiguities.ConsoleWriter">
    </bean>

    <bean id="filewriter"
          class="com.spring.autowire.removeambiguities.FileWriter">
    </bean>

    <bean id="squirrel"
          class="com.spring.autowire.removeambiguities.FileWriter"
          autowire-candidate="false">
    </bean>

</beans>